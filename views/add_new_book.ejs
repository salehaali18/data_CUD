<body>
<center>
        <form action="<%if(editable){%>/book_update<%}else{%>/book_save<%}%>" method="post" enctype="multipart/form-data"> 
            
            <p class="lead">Welcome Back! ....
                     BOOK DETAILS</p>
            <div class="form-group">
			    <input  type="text" name="title" class="required form-control"  id="name" value="<%if(editable){%><%=data.title%><%}%>" placeholder="Title">
            </div>
            <div class="form-group">
                    <input  type="text" name="book_name" class="required form-control"  id="book_name" value="<%if(editable){%><%=data.book_name%><%}%>"placeholder="Book_Name">
                </div>
                <div class="form-group">
                        <input  type="text" name="book_publication" class="required form-control"  id="book_publication"  value="<%if(editable){%><%=data.book_publication%><%}%>" placeholder="book_publication">
                    </div>
                    <div class="form-group">
                            <input  type="text" name="book_author" class="required form-control"  id="book_author"  value="<%if(editable){%><%=data.book_author%><%}%>" placeholder="book_author">
                        </div>

                        <div class="form-group">
                          <input  type="file" name="image" class="required form-control"  id="image"   placeholder="images">
                      </div>
                      <input type="hidden" name="_csrf" value="<%=csrftoken%>">
                        <input type="hidden" name="id" value="<%if(editable){%><%=data._id%><%}%>">
                        
           
           <br><br> <button type="submit"><%if(editable){%>Book Update<%}else{%>Book Add<%}%></button></center>
        </form>
    
